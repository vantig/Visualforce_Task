<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Working_Hours</fullName>
    <active>true</active>
    <description>Check Working Hours</description>
    <errorConditionFormula>Working_Hours_Start__c &gt; Working_Hours_End__c</errorConditionFormula>
    <errorMessage>Start of working hours cannot be later, then the End of working hours</errorMessage>
</ValidationRule>
